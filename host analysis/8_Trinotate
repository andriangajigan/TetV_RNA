$ /opt/apps/software/bio/Trinotate/3.2.2-intel-2018.5.274/auto/autoTrinotate.pl 
--Trinotate_sqlite Trinotate.sqlite 
--gene_to_trans_map Trinity.fasta.gene_trans_map #TRinity output
--transcripts Trinity_tet_all_noV_good_hform.fasta 
--conf conf_tet_host.txt #configuration file
